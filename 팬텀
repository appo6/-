---- KeyBind ----

mouse = game.Players.LocalPlayer:GetMouse()

local function onKeyDown( key )
	if key:lower() == "x" then
		if MainGUI.Visible == true then
			MainGUI.Visible = false
		else
			MainGUI.Visible = true
		end
	end
end
mouse.KeyDown:connect(onKeyDown)
