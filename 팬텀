-- Scripts:
function SCRIPT_IUMC90_FAKESCRIPT() -- username.Script 
	local script = Instance.new('Script')
	script.Parent = username
	while wait() do
	local r = (math.sin(workspace.DistributedGameTime/2)/2)+0.5
	local g = (math.sin(workspace.DistributedGameTime)/2)+0.5
	local b = (math.sin(workspace.DistributedGameTime*1.5)/2)+0.5
	local color = Color3.new(r, g, b)
	script.Parent.BorderColor3 = color
	end

end
coroutine.resume(coroutine.create(SCRIPT_IUMC90_FAKESCRIPT))
function SCRIPT_AUJH90_FAKESCRIPT() -- password.Script 
