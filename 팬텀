clicktp.Name = "clicktp"
clicktp.Parent = scripts
clicktp.BackgroundColor3 = Color3.new(0.313726, 0.313726, 0.313726)
clicktp.Position = UDim2.new(-1.94108891, 0, 1.61044466, 0)
clicktp.Size = UDim2.new(0, 75, 0, 52)
clicktp.Font = Enum.Font.SourceSans
clicktp.Text = "Clicktp (ctrl)"
clicktp.TextColor3 = Color3.new(1, 1, 1)
clicktp.TextScaled = true
clicktp.TextSize = 14
clicktp.TextWrapped = true
clicktp.MouseButton1Down:connect(function()
	local UIS = game:GetService("UserInputService")

local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()


function GetCharacter()
   return game.Players.LocalPlayer.Character
end

function Teleport(pos)
   local Char = GetCharacter()
   if Char then
       Char:MoveTo(pos)
   end
end


UIS.InputBegan:Connect(function(input)
   if input.UserInputType == Enum.UserInputType.MouseButton1 and UIS:IsKeyDown(Enum.KeyCode.LeftControl) then
       Teleport(Mouse.Hit.p)
   end
end)
end)

