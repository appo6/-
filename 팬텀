loginbutton.MouseButton1Down:connect(function()
	if username.Text == "New Trial" and password.Text == "For Free" then
		MainGUI.Visible = true
		loginmain.Visible = false
		local message = Instance.new("Message",game.Workspace)
   message.Text = 'Succesfully loaded! Add me on discord (T0mGames#0477) for access to our discord server!'
   wait(6)
   message:Destroy()
	local message = Instance.new("Message",game.Workspace)
   message.Text = 'The keybind to open and close is x!'
   wait(3)
   message:Destroy()
	else
		loginbutton.Text = "Incorrect login. Try again"
		wait(1.5)
		loginbutton.Text = "Login"
	end
end)

